@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}


<div class="card mt-5">
    <div class="d-flex justify-content-between">
        <h1 class="mb-3">@Model.TableHeader()</h1> <a asp-page="Create" class="btn btn-info h-25 me-2 fw-bold text-white" style="width: 150px; border: 2px solid #000032"><i class="fas fa-check-circle text-white me-1"></i>Create</a>
    </div>

    @*@foreach (var item in Model.IntList())

    {               

        <h1>@item</h1>

    }*@


    <table id="formTable" class="table table-striped table-bordered display compact responsive mt-5 mb-3">
        <thead>
            <tr class=" text-info text-center" style="background: #000032 ">
                <th>Id</th>
                <th>Name</th>
                <th>Date Worked</th>
                <th>Notes</th>
                <th>Category</th>
                <th>Edit</th>
            </tr>
        </thead>
    </table>
</div>


@section Scripts{

    <script src='~/js/Tables/FormTable.js'></script>



}